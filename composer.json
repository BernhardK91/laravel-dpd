{
    "name": "bernhardk/laravel-dpd",
    "description": "Laravel package for using DPD Webservices to submit shipment to dpd und retrieve its label and tracking information.",
    "keywords": ["laravel", "dpd", "dpd cloud service", "webservices"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bernhard Kraemer",
            "email": "mail@bernhard-kraemer.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.0.2",
        "ext-soap": "*",
        "illuminate/support": "^6|^7|^8|^9|^10|^11"
    },
    "autoload": {
        "psr-4": {
            "BernhardK\\Dpd\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BernhardK\\Dpd\\DpdServiceProvider"
            ]
        }
    }
}
